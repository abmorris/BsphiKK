    <PhysicsParameter>
      <Name>DwaveFrac</Name>
      <Value>1.5</Value>
      <Minimum>0.0</Minimum>
      <Maximum>10.0</Maximum>
      <Type>Fixed</Type>
      <Unit>Unitless</Unit>
    </PhysicsParameter>
    # Magnitudes squared
    <PhysicsParameter>
      <Name>APperp2</Name>
      <Value>0.365</Value>
      <Minimum>0.0</Minimum>
      <Maximum>1.0</Maximum>
      <Type>Fixed</Type>
      <Unit>Unitless</Unit>
    </PhysicsParameter>
    <PhysicsParameter>
      <Name>APzero2</Name>
      <Value>0.348</Value>
