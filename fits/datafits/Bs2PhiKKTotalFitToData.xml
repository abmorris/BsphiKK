<RapidFit>
  <ParameterSet>
    <PhysicsParameter>
      <Name>SignalFraction</Name>
      <Value>0.7</Value>
      <Minimum>0.6</Minimum>
      <Maximum>0.9</Maximum>
      <Type>Fload</Type>
    </PhysicsParameter>
    <PhysicsParameter>
      <Name>A</Name>
      <Value>1.97009</Value>
      <Type>Fixed</Type>
    </PhysicsParameter>
    <PhysicsParameter>
      <Name>B</Name>
      <Value>-3.50295</Value>
      <Type>Fixed</Type>
    </PhysicsParameter>
    <PhysicsParameter>
      <Name>C</Name>
      <Value>10.3624</Value>
      <Type>Fixed</Type>
    </PhysicsParameter>
    <PhysicsParameter>
      <Name>M</Name>
      <Value>986.115</Value>
      <Type>Fixed</Type>
    </PhysicsParameter>
    # Scale factors for each component
    <PhysicsParameter>
      <Name>SwaveFrac</Name>
      <Value>0.2</Value>
      <Minimum>0.0</Minimum>
      <Maximum>0.4</Maximum>
      <Type>Float</Type>
      <Unit>Unitless</Unit>
    </PhysicsParameter>
    <PhysicsParameter>
      <Name>PwaveFrac</Name>
