      <Type>Fixed</Type>
      <Unit>Unitless</Unit>
    </PhysicsParameter>
    # Phases
    <PhysicsParameter>
      <Name>deltaSzero</Name>
      <Value>1.15</Value> # Jpsi KK value
      <Minimum>-3.14159</Minimum>
      <Maximum>3.14159</Maximum>
      <Type>Fixed</Type>
      <Unit>Unitless</Unit>
    </PhysicsParameter>
    <PhysicsParameter>
      <Name>deltaPperp</Name>
      <Value>2.39</Value>
      <Minimum>-3.14159</Minimum>
      <Maximum>3.14159</Maximum>
      <Type>Fixed</Type>
      <Unit>Unitless</Unit>
    </PhysicsParameter>
    <PhysicsParameter>
      <Name>deltaPzero</Name>
      <Value>0.0</Value>
      <Minimum>-3.1415927</Minimum>
      <Maximum>3.1415927</Maximum>
      <Type>Fixed</Type>
      <Unit>Unitless</Unit>
    </PhysicsParameter>
    <PhysicsParameter>
      <Name>deltaPpara</Name>
      <Value>2.71</Value>
      <Minimum>-3.1415927</Minimum>
      <Maximum>3.1415927</Maximum>
      <Type>Fixed</Type>
      <Unit>Unitless</Unit>
    </PhysicsParameter>
    <PhysicsParameter>
      <Name>deltaDperp</Name>
      <Value>0.0</Value>
      <Minimum>-3.14159</Minimum>
      <Maximum>3.14159</Maximum>
      <Type>Fixed</Type>
      <Unit>Unitless</Unit>
    </PhysicsParameter>
    <PhysicsParameter>
      <Name>deltaDzero</Name>
      <Value>0.0</Value>
      <Minimum>-3.1415927</Minimum>
      <Maximum>3.1415927</Maximum>
