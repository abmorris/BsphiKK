			<Observable>
				<Name>cosTheta</Name>
				<TF1>trcostheta</TF1>
				<Minimum>-1.0</Minimum>
				<Maximum>1.0</Maximum>
				<Unit> </Unit>
			</Observable>
			<Observable>
				<Name>phi</Name>
				<TF1>trphi</TF1>
				<Minimum>-3.1416</Minimum>
				<Maximum>3.1416</Maximum>
				<Unit>rad</Unit>
			</Observable>
			<Observable>
				<Name>cosPsi</Name>
				<TF1>trcospsi</TF1>
				<Minimum>-1.0</Minimum>
				<Maximum>1.0</Maximum>
				<Unit> </Unit>
			</Observable>
			<Observable>
				<Name>tag</Name>
				<TF1>trueid/531.</TF1>
				<Value>1.</Value>
				<Value>-1.</Value>
				<Unit> </Unit>
			</Observable>
			<Observable>
				<Name>mistag</Name>
				<TF1>0.</TF1>
				<Minimum>0.</Minimum>
				<Maximum>0.5</Maximum>
				<Unit> </Unit>
			</Observable>
