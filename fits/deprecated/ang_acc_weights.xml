	</CommonPhaseSpace>

	<ToFit>
		<CommonPDF>True</CommonPDF>
		<DataSet>
			<Source>File</Source>
			#<FileName>/afs/cern.ch/user/d/dferguso/Bs2Psi2SPhi/fit/data_files/MC2012_psi2s_output.root</FileName>
			<FileName>/afs/cern.ch/user/d/dferguso/Bs2Psi2SPhi/fit/data_files/MC2011_psi2s_output.root</FileName>
